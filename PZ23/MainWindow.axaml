<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="PZ23.MainWindow"
        Title="Главное окно"
        Background="Pink">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Добавить заявку" Width="150" Background="White" Click="AddRequestWindowOnClick" />
        <Button Content="Все заявки" Width="150" Background="White" Margin="0 10 0 0" Click="RequestsWindowOnClick"/>
    </StackPanel>
</Window>
